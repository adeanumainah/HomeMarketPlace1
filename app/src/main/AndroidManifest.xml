<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.dean.homemarketplace">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.CALL_PHONE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/estate2"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.HomeMarketPlace">
        <activity android:name=".activity.AuthenticationHpActivity"></activity>
        <activity android:name=".activity.WelcomeActivity" />
        <activity android:name=".activity.SeeAllTerkiniActivity" />
        <activity android:name=".activity.SeeAllPopularActivity" />
        <activity android:name=".activity.SplashScreenActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.DetailActivity" />
        <activity android:name=".activity.SignUpActivity" />
        <activity android:name=".activity.SignInActivity" />
        <activity
            android:name=".activity.MainActivity"
            android:label="@string/app_name" />
    </application>

</manifest>